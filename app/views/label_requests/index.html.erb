<p id="notice"><%= notice %></p>

<h1>Label Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @label_requests.each do |label_request| %>
      <tr>
        <td><%= link_to 'Show', label_request %></td>
        <td><%= link_to 'Edit', edit_label_request_path(label_request) %></td>
        <td><%= link_to 'Destroy', label_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Label Request', new_label_request_path %>
